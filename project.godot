config_version=5

[application]

config/name="Orgod"
run/main_scene="res://Scenes/Menu/Main_menu.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
run/max_fps=60
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
config/icon="res://Assets/Antag.png"
config/windows_native_icon="res://Assets/File_icon/Antag.ico"

[autoload]

Base="*res://Script/Based/Base.gd"
SpellsDB="*res://Script/Based/SpellsDB.gd"
ItemsDB="*res://Script/Based/ItemsDB.gd"
Enums="*res://Script/Based/Enums.gd"
AbilitiesDB="*res://Script/Based/AbilitiesDB.gd"
BuildDB="*res://Script/Based/BuildDB.gd"
LvlupDB="*res://Script/Based/LvlupDB.gd"
CreepsDB="*res://Script/Based/CreepsDB.gd"
HeroesDB="*res://Script/Based/HeroesDB.gd"
Lobby="*res://Script/Multiplayer/Lobby.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/initial_position_type=0
window/size/window_width_override=1600
window/size/window_height_override=850
window/stretch/mode="viewport"
window/vsync/vsync_mode=0

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_state_charts/plugin.cfg")

